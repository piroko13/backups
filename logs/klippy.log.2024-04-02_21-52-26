[gcode_macro START_PRINT]
gcode = 
	BEEP
	G92 E0
	G28
	STATUS_HEATING
	M104 S{180}
	M140 S{50}
	M190 S{50}
	BED_MESH_CALIBRATE
	
	SMART_PARK
	M104 S{params.EXTRUDER_TEMP}
	M140 S{params.BED_TEMP}
	STATUS_HEATING
	M190 S{params.BED_TEMP}
	LINE_PURGE
	STATUS_PRINTING
	G92 E0
	G1 Z2.0 F3000
	G1 X5 Y20 Z0.3 F5000.0
	M117 Printing...

[gcode_macro END_PRINT]
