	STATUS_HEATING
	M140 S60
	M117 Heating bed to remove print...
	BEEP
	M190 S60
	BEEP_TWICE
	M117 Done heating!!!

[gcode_macro START_PRINT]
gcode = 
	BEEP
	G92 E0
	G28
	STATUS_HEATING
	M104 S{180}
	M140 S{50}
	M190 S{50}
	BED_MESH_CALIBRATE
	
	SMART_PARK
	M104 S{params.EXTRUDER_TEMP}
	M140 S{params.BED_TEMP}
	STATUS_HEATING
	M190 S{params.BED_TEMP}
	LINE_PURGE
	STATUS_PRINTING
	G92 E0
	G1 Z2.0 F3000
	G1 X5 Y20 Z0.3 F5000.0
	M117 Printing...

[gcode_macro END_PRINT]
gcode = 
	BEEP_TWICE
	G91
	G1 E-5 F2700
	G1 E-2 Z0.2 F2400
	G1 X5 Y5 F3000
	G1 Z10
	G90
	G1 X0 Y210
	M106 S0
	M104 S0
	M140 S0
	M84 X Y E
	BED_MESH_CLEAR
	STATUS_PART_READY
	M117 Done!!!

